{
  "title": "jk to escape",
  "maintainers": [
    "raeperd"
  ],
  "homepage": [
    "https://raeperd.github.io"
  ],
  "rules": [
    {
      "description": "jk to escape",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "simultaneous": [
              {
                "key_code": "j"
              },
              {
                "key_code": "k"
              }
            ],
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "escape"
            }
          ]
        }
      ]
    }
  ]
}
